{
    "identifier": "desktop",
    "windows": [
        "main"
    ],
    "permissions": [
        "core:default",
        "allow-window-commands",
        "allow-tor-commands",
        "allow-misc-commands",
        "allow-relay-commands",
        "allow-wallet-commands",
        "shell:allow-kill",
        "shell:allow-open",
        "http:default",
        "upload:default",
        "fs:default",
        "fs:allow-write-file",
        "fs:allow-remove",
        "fs:allow-temp-write-recursive",
        "dialog:default",
        {
            "identifier": "shell:allow-spawn",
            "allow": [
                {
                    "sidecar": "bin/tor"
                }
            ]
        }
    ]
}